{
  "name": "roxblnfk/cycle-active-record",
  "description": "Cycle Active Record",
  "keywords": [
    "roxblnfk",
    "spiral",
    "cycle-active-record"
  ],
  "homepage": "https://github.com/roxblnfk/cycle-active-record",
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksei Gagarin",
      "email": "roxblnfk@ya.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "cycle/orm": "2.0",
    "spiral/boot": "^2.9",
    "spiral/console": "^2.9"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^9.5",
    "spiral/testing": "^1.0",
    "vimeo/psalm": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "Cycle\\AR\\CycleActiveRecord\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cycle\\AR\\CycleActiveRecord\\Tests\\App\\": "tests/app",
      "Cycle\\AR\\CycleActiveRecord\\Tests\\": "tests/src"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm --config=psalm.xml ./src"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "spiral": {
      "bootloaders": [
        "Cycle\\AR\\CycleActiveRecord\\Bootloader\\CycleActiveRecordBootloader"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
